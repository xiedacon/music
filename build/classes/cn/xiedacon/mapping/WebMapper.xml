<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="cn.xiedacon.dao.WebDao">

	<sql id="commonEnd">
		FROM websitesetting w
		WHERE id = 1
	</sql>

	<select id="selectAblumIds" resultType="string">
		SELECT w.ablum 
		<include refid="commonEnd" />
	</select>

	<select id="selectSongMenuIds" resultType="string">
		SELECT w.songMenu 
		<include refid="commonEnd" />
	</select>
	
	<select id="selectSongMenuSecondTagIds" resultType="string">
		SELECT w.songMenuSecondTag
		<include refid="commonEnd" />
	</select>
	
	<select id="selectUserIds" resultType="string">
		SELECT w.user
		<include refid="commonEnd" />
	</select>
	
	<select id="selectSongListIds" resultType="string">
		SELECT w.songList
		<include refid="commonEnd" />
	</select>
</mapper>