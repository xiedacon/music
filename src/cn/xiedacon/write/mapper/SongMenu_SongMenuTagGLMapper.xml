<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.xiedacon.write.dao.SongMenu_SongMenuTagGLDao">

	<update id="deleteBySongMenuId">
		DELETE FROM smi_gl_rsms
		WHERE songMenuId = #{songMenuId}
	</update>

	<insert id="insert" parameterType="cn.xiedacon.model.SongMenu_SongMenuTagGL">
		INSERT INTO song_gl_songmenu
		VALUES(
		#{songMenuId},
		#{songId},
		#{time}
		)
	</insert>
</mapper>